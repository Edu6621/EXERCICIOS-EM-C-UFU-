// 4. faca um programa que leia um nome e imprima as 4 primeiras letras do nome.
#include <stdio.h>
#include <string.h>
int main () {
    char string[50];
    
    printf ("informe um nome : ");
    fgets (string, sizeof(string), stdin);
    for (int i=0; i<4; i++){
        printf ("%c", string[i]);
    }
}

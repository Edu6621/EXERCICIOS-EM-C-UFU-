//11. Faca um programa que receba do usuario uma string. O programa imprime a string sem
//suas vogais
#include <stdio.h>
#include <string.h>

int main () {
    char string[50];
    int i=0;
    
    printf ("informe uma string : ");
    fgets (string, sizeof(string), stdin);
    while (string[i] != '\0'){
        if (string[i] == 'a'
        || string[i] == 'e'
        || string[i] == 'i'
        || string[i] == 'o'
        || string[i] == 'u'
        || string[i] == 'A'
        || string[i] == 'E'
        || string[i] == 'I'
        || string[i] == 'O'
        || string[i] == 'U'){
        } else {
            printf ("%c", string[i]);
        }
        i++;
    }
}

// 9. Crie um programa que contenha uma matriz de float contendo 3 linhas e 3 colunas.
// Imprima o endereco de cada posicao dessa matriz.
#include <stdio.h>
int main () {
    float mat[3][3];
    for (int i=0; i<3; i++){
        for (int j=0; j<3; j++){
            printf ("O endereço de [%i][%i] é : %p\n", i, j, &mat[i][j]);
        }
    }
    return 0;
}

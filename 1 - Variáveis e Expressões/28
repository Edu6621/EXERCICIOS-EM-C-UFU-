/*Faca a leitura de tres valores e apresente como resultado a soma dos quadrados dos
tres valores lidos.*/
#include <stdio.h>
void main () {
    float a, b, c, d;
    printf ("informe 3 valores :\n ");
    scanf ("%f",&a);
    scanf ("%f",&b);
    scanf ("%f",&c);
    d=(a*a)+(b*b)+(c*c);
    printf ("a soma dos quadrados dos 3 valores eh :%.2f\n ", d);
}

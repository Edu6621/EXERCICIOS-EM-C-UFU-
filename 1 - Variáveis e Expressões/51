/*  Escreva um programa que leia as coordenadas x e y de pontos no R2 e calcule sua
    distancia da origem ˆ (0, 0).                                                   */
#include <stdio.h>
#include <math.h>
int main () {
    float o, x, y;
    printf ("insira o valor de x : ");
    scanf ("%f", &x);
    printf ("insira o valor de y : ");
    scanf ("%f", &y);
    o=(sqrt((x*x)+(y*y)));
    printf ("a distância de origem é : %.2f ", o);
    return 0;
}

// 57. Faca um programa que conte quantos numeros primos existem entre a e b, onde a e b
//sao numeros informados pelo usuario. 
#include <stdio.h>
#include <math.h>
int main () {
    int a, b;
    int qtd=0;
    
    printf ("informe a : ");
    scanf ("%i", &a);
    printf ("informe b : ");
    scanf ("%i", &b);
    
    for (int i=a; i<b; i++){
        int cont=0;
        for (int j=2; j<=sqrt(i); j++){
            if (i%j==0){
                cont++;
                break;
            }
        }
        if (cont==0){
            qtd++;
        }
    }    
    printf ("a quantidade Ã© : %i", qtd-1);
    return 0;
}

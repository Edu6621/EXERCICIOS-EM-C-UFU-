//5. Faça um programa que peça ao usuario para digitar 10 valores e some-os.
#include <stdio.h>
int main () {
    int valor;
    int soma = 0;
    int quantidade = 1;
    while (quantidade <=10){
        printf ("%i ) digite um valor : ", quantidade);
        scanf ("%i", &valor);
        quantidade++;
        soma=valor+soma;
        
    }printf ("a soma é %i", soma);
    return 0;
}

// 56. Faca um programa que calcule a soma de todos os numeros primos abaixo de dois milhoes.
#include <stdio.h>
#include <math.h>
int main () {
    int n=2000000;
    long long soma=0;
    
    for (int i=2; i<n; i++){
        int cont=0;
        for (int j=2; j<=sqrt(i); j++){
            if (i%j==0){
                cont++;
                break;
            }
        }
        if (cont==0){
            soma+=i;
        }
    }    
    printf ("a soma Ã© %lli", soma);
    return 0;
}

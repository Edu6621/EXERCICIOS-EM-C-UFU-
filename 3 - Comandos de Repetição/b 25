/* 25. Faca um programa que some todos os numeros naturais abaixo de 1000 que sao multiplos
de 3 ou 5. */
#include <stdio.h>
int main (){
    int numero;
    int soma = 0;
    int contador = 1;
    
    while (contador >=1 && contador < 1000 ){
        if (contador%5==0 || contador%3==0){
            printf ("%i ", contador);
            soma=contador+soma;
        }
        contador++;
    }printf("\n%i", soma);
}

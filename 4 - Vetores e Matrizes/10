/* 10. Faca um programa para ler a nota da prova de 15 alunos e armazene num vetor, calcule
e imprima a media geral.  */
#include <stdio.h>
int main () {
    float vet[15], soma=0, media=0;
    for (int i=0; i<15; i++){
        printf ("%i ) informe a nota da prova : ",i+1);
        scanf ("%f", &vet[i]);
    }
    for (int i=0; i<15; i++){
        soma+=vet[i];
    }
    media=soma/15.0;
    printf ("a media geral Ã© : %.2f", media);
}

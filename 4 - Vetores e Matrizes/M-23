// 23. Faca um programa que leia uma matriz A de tamanho 3 x 3 e calcule B = A^2

#include <stdio.h>

int main () {
    int A[3][3];
    int B[3][3] = {0};
    for (int i=0; i<3; i++){
        for (int j=0; j<3; j++){
            printf ("informe o valor de [%d][%d] : ", i, j);
            scanf("%d", &A[i][j]);
        }
    }
    for (int i=0; i<3; i++){
        for (int j=0; j<3; j++){
            for (int k=0; k<3; k++){
                B[i][j] += A[i][k] * A[k][j];
            }
        }
    }
    for (int i=0; i<3; i++){
        for (int j=0; j<3; j++){
            printf ("[%d]", B[i][j]);
        }
        printf ("\n");
    }
    return 0;
}

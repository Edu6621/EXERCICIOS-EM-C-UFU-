/*15. Leia um vetor com 20 numeros inteiros. Escreva os elementos do vetor eliminando ele-
mentos repetidos. */
#include <stdio.h>
int main () {
    int vet[20];
    for (int i=0; i<20; i++){
        printf ("%i ) informe um nÃºmero : ", i+1);
        scanf ("%i", &vet[i]);
    }
    for (int i=0; i<20; i++){
        printf ("%i ", vet[i]);
    }
    printf ("\n");
    for (int i=0; i<20; i++){
        for (int j=0; j<20; j++){
            if (i!=j){
                if (vet[i]==vet[j]){
                    vet[i]=0;
                    vet[j]=0;
                }
            }
        }
    }
    for (int i=0; i<20; i++){
        if (vet[i]!=0){
            printf ("%i ", vet[i]);
        }
    }    
    
}

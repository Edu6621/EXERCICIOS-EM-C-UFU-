/* 18. Faca um programa que leia um vetor de 10 numeros. Leia um numero x. Conte os
multiplos de um numero inteiro x num vetor e mostre-os na tela. */
#include <stdio.h>
int main () {
    int vet[10], x;
    for (int i=0; i<10; i++){
        printf ("%i ) informe um número : ", i+1);
        scanf ("%i", &vet[i]);
    }
    printf ("informe um número : ");
    scanf ("%i", &x);
    for (int i=0; i<10; i++){
        if (vet[i]%x==0){
            printf ("%i é múltiplo de %i\n", vet[i], x);
        }
    }
}

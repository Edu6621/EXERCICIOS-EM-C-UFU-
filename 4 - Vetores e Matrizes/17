/* 17. Leia um vetor de 10 posicoes e atribua valor 0 para todos os elementos que possuÄ±rem
valores negativos */
#include <stdio.h>
int main () {
    int vet[10];
    for (int i=0; i<10; i++){
        printf ("%i ) informe um valor : ", i+1);
        scanf ("%i", &vet[i]);
        if (vet[i]<0){
            vet[i]=0;
        }
    }
    for (int i=0; i<10; i++){
        printf ("%i ", vet[i]);
    }    
}

//2. Crie um programa que le 6 valores inteiros e, em seguida, mostre na tela os valores lidos.
#include <stdio.h>
int main () {
    int vet[6];
    for (int i=0; i<6; i++){
        printf ("%i ) informe um número : ", i+1);
        scanf ("%i", &vet[i]);
    }
    printf ("os valores lídos são : ");
    for (int j=0; j<6; j++){
        printf ("%i ", vet[j]);
    }
}

// 25. Faca um programa que preencha um vetor de tamanho 100 com os 100 primeiros naturais que nao
// sao multiplos de 7 ou que terminam com 7. 
#include <stdio.h>
int main () {
    int vet[100];
    int cont=0, i=0;
    while (cont<100){
        if (i%7 != 0 && i%10 != 7){
            vet[cont] = i;
            cont++;
        }
        i++;
    }
    for (int i=0; i<100; i++){
        printf ("%i ", vet[i]);
    }
    return 0;
}
